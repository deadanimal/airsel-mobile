ilter = this.url + \"?\" + field;\n    return this.http.get<WorkActivitiesModel[]>(urlFilter).pipe(\n      tap((res) => {\n        console.log(\"WorkActivitiesModel\", res);\n      })\n    );\n  }\n}\n"],"sourceRoot":"webpack:///"}